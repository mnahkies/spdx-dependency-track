{
  "name": "spdx-dependency-track",
  "version": "0.1.0",
  "packageManager": "yarn@4.1.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "husky",
    "dev": "next dev",
    "build": "next build",
    "package": "docker build -t spdx-dependency-track --progress=plain .",
    "start": "next start",
    "lint": "next lint",
    "format": "biome check --apply ./src && biome format --write ./src",
    "docs:generate": "yarn markdown-toc -i --bullets=- ./README.md",
    "migrate": "dotenvx run -- ./bin/migrate.sh",
    "migrate-generate": "dotenvx run -- ./bin/migrate-generate.sh",
    "migrate-rehash": "dotenvx run -- atlas migrate hash --dir 'file://migrations"
  },
  "dependencies": {
    "@ariga/atlas": "^0.21.0",
    "@dotenvx/dotenvx": "^0.27.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/material-nextjs": "^5.15.11",
    "@nahkies/typescript-fetch-runtime": "^0.8.0",
    "@nahkies/typescript-koa-runtime": "^0.8.0",
    "better-sqlite3": "^9.5.0",
    "next": "14.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "semver": "^7.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@nahkies/openapi-code-generator": "^0.8.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semver": "^7",
    "eslint": "^8",
    "eslint-config-next": "14.2.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markdown-toc": "^1.2.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --cache --report-unused-disable-directives --fix",
      "yarn biome check --apply --no-errors-on-unmatched"
    ],
    "*.md": [
      "sh -c 'yarn docs:generate'"
    ]
  }
}
